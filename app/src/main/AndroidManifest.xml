<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.covidtracker">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/o"
        android:label="@string/app_name"
        android:roundIcon="@drawable/o"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".NewsDetails"></activity>
        <activity android:name=".News" />
        <activity android:name=".Settings" />
        <activity android:name=".Notification" />
        <activity android:name=".Help" />
        <activity android:name=".Protection" />
        <activity
            android:name=".custom"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity android:name=".Symptoms" />
        <activity android:name=".Details" />
        <activity android:name=".Affectedcountry" />
        <activity android:name=".Data" />
        <activity android:name=".Home" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>